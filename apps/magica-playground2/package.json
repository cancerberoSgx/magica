{
  "name": "magica-playground2",
  "version": "0.0.1",
  "description": "magica playground version 2",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "clean": "rm -fr dist/",
    "build-ts": "tsc",
    "build": "npm run clean && npm run build-ts",
    "start": "npm run copy; parcel src/index.html",
    "copy": "mkdir dist; cp -r node_modules/magica/src/imageMagick/compiled/magick.wasm src/assets/* dist",
    "format": "npx ts-refactor format \"src/**/*.ts*\" ./formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk",
    "organizeImports": "npx ts-refactor organizeImports \"src/**/*.ts*\" \"test/**/*.ts*\" \"test-browser/**/*.ts*\"  ./formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk ",
    "lint": "npm run organizeImports && npm run format"
  },
  "author": "Sebasti√°n Gurin",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.9.4",
    "file-saver": "^2.0.5",
    "magica": "file:../..",
    "magica-examples": "file:../magica-examples",
    "misc-utils-of-mine-generic": "^0.2.43",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.13.8",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "parcel": "^1.12.3",
    "sass": "^1.52.3",
    "ts-node": "^10.8.1",
    "ts-refactor": "0.0.11",
    "typescript": "^4.7.3"
  }
}
